{
  "name": "waldemar-neto-nodejs-typescript",
  "version": "1.0.0",
  "description": "Seguindo curso do waldemar sobre Api Typescript yarn e module-alias",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "start": "npm run build && node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only src/index.ts  | pino-pretty -c",
    "test": "npm run test:unit && npm run test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest --maxWorkers=1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/jest": "^29.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "^18.13.0",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.12",
    "jest": "^29.4.3",
    "nock": "^13.3.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^7.0.0",
    "pino": "^8.11.0"
  }
}
